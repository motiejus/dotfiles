export GPG_SSH_AUTH_SOCK=$(gpgconf --list-dirs agent-ssh-socket)
gpgconf --launch gpg-agent

if [[ false ]]; then
    xrdb -merge $HOME/.Xresources
fi

. $HOME/.profile

nm-applet &

if [ $(hostname) != motiejus-20NYS8QG0B ]; then
  blueman-applet &
fi

pnmixer &
compton --daemon
kdeconnect-indicator &

xss-lock -- slock xset dpms force off &
autorandr -c
